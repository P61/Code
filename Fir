# for遍历列表
s = ['a','b','c','d']
print('修改前:',s)

# i = 0
# while i< len(s) :
#     print( s[i] )
#     i+=1

# for循环
# for 变量 in 序列 :
#     代码块

for n in s :# 序列s的元素依次赋值给变量n
    print( n )
 
 # range 生成一个自然数的序列
r = range( 5 )
print( list ( r ) )
r = range( 0,10,2 )
print( list ( r ) )
r = range( 10,0,-1 )
print( list ( r ) )

for i in range(0,6) :
    print(i)
